<script type="text/javascript"> 
var server = '';

/***/
$(document).ready(function() {
	var 
		path = $(location).attr('pathname').split(/\//g),
		delimiter = ' > ',
		link = '',
		li = '',
		excluded = ['edit', 'view', 'duplicate', 'edit']
		;
//	$('.breadcrumb').append()	
	$.each(path, function(i, val){
		if(i > 0 && excluded.indexOf( val ) === -1 ){ 
			link = $('<a></a>').attr('href', '/'+path.slice(1,i+1).toString().replace(/,/g,'/') ).text( val );
			li = $('<li></li>');
			if( i < path.length -1 ){
				li.append(link).addClass('active');//.append(delimiter);
			}else{
				li.append( val );
			}
			$('.breadcrumb').append(li)
		}
	});
});

/***/
var render = function(template, path, remote, selector){ 
		$('.main').hide(); 
  	require(['text!../views/'+template+'.ejs', 'ejs'], function(thetemplate) {  
  		if(remote){        
				$.ajax({
		      type: "get",
		      beforeSend: function(xhr){ 
		  				if (xhr.overrideMimeType){
						    xhr.overrideMimeType("application/json");
		  				}
					},
		      url: server + path,
		      dataType: 'json',
		      success: function(data){ 
						var html = ejs.render(thetemplate, { items: data });
						$(selector).html(html);
					},
					error : function(e){ console.log('ERROR at '+template+':'+JSON.stringify(e)); }		  
				});
			
			}else{ 
				var html = ejs.render(thetemplate, {items:0});
				$(selector).html(html);
			}	
	}); // end require
};	// end render

$(window).load(function () {
	render('admin-user-data', '/json/user-data', true, '.user-area'); 
}); 


// render markdown
		$('.mark').each(function(i, val){
				$(val).html(markdown.toHTML($(val).text()))
		});

</script>


